<Window x:Class="SpaceInvaders.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpaceInvaders"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525"
        KeyDown="Windows_KeyDown"
        KeyUp="Windows_KeyUp">
    <Grid>
        <Canvas x:Name="space" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Black">
            <TextBox x:Name="textBox" Height="17" Canvas.Left="10" TextWrapping="Wrap" Text="0" Canvas.Top="10" Width="40" RenderTransformOrigin="0.504,0.471" Foreground="#FFFFF9F9" BorderBrush="Black" Background="Black" SelectionBrush="Black" FontSize="10" FontFamily="Segoe WP Black" IsReadOnlyCaretVisible="True"/>
        </Canvas>

    </Grid>
</Window>
