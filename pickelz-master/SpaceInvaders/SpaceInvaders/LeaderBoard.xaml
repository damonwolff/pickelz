<Window x:Class="SpaceInvaders.LeaderBoard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpaceInvaders"
        mc:Ignorable="d"
        Title="LeaderBoard" Height="305.19" Width="288.581">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Resources/space.jpg"/>
        </Grid.Background>
        <TextBox x:Name="highScoreTxtBox" HorizontalAlignment="Left" Height="229" Margin="10,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="261"/>
        <Button x:Name="button" Content="Back" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
    </Grid>
</Window>
