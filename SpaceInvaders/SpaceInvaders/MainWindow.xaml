<Window x:Class="SpaceInvaders.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpaceInvaders"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525"
        KeyDown="Windows_KeyDown"
        KeyUp="Windows_KeyUp">
    <Grid>
        <Canvas x:Name="space" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Black">
            <Image x:Name="ship" Height="21" Source="ship.png" Stretch="Fill" Canvas.Bottom="10" Width="33" Canvas.Left="239"/>
        </Canvas>

    </Grid>
</Window>
